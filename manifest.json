{
  "manifest_version": 2,

  "name": "Favorite Place Navigator",
  "description": "This extension remembers the places you wish you visit and reminds you if it is on your way to somewhere",
  "version": "1.0",

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*.google.com/*","*://*.yelp.com/*"],
    "js": ["jquery-3.2.1.min.js", "content.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "browser_action": {
    "default_title": "Test Extension"
  },  "permissions": [
    "activeTab",
	"alarms",
    "storage",
    "contextMenus",
    "http://*/"
  ],
  "icons": {
    "16": "icon.png"
  }
}